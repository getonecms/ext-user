{
    "name": "getonecms/user",
    "description": "OneCMS user extension",
    "type": "onecms-ext",
    "authors": [
        {
            "name": "Mohammed Shifreen",
            "email": "mshifreen@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "OneCMS\\User\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OneCMS\\User\\Test\\": "test/"
        }
    },
    "optimize-autoloader": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}